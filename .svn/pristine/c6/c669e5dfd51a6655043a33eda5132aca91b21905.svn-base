.set-list{
  width:100%;
  box-sizing: border-box;
  letter-spacing: 0.2rem;
}
.set-list>view{
  padding:0 40rpx;
  box-sizing: border-box;
}
.set-list>view:first-child{
  width:100%;
  height:120rpx;
  line-height: 150rpx;
  font-size:40rpx;
  color:#ADADAD;
}
.set-list>view:not(:first-child){
  background-color: #fff;
  height:100rpx;
  line-height: 100rpx;
  font-size:32rpx;
}
.set-list>.set-item:not(:last-child){
  margin-top:20rpx;
}
.set-list>.set-item:last-child{
  margin-top:60rpx;
}
.set-item>view{
  width:50%;
}