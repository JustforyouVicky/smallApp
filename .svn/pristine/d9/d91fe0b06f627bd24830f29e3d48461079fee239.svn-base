.photo-list {
  padding: 30rpx;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.photo-list .photo-item {
  width: 32%;
  height: 200rpx;
  margin-bottom: 20rpx;
}

.photo-list .photo-item image {
  width: 100%;
  height: 100%;
}
.show{
  display:block;
}
.hide{
  display:none;
}
.model{
  /* display:none; */
  position: fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,1);
}
.model image{
  width:100%;
  /* height: */
  position: absolute;
  top:0;
  right:0;
  bottom:0;
  left:0;
  margin:auto;

}