.canvas-first,.canvas-second,
.canvas-third,.canvas-last{
  width:100%;
  height:100%;
  position: relative;
}
.canvas-first>image,
.canvas-second>image,
.canvas-third>image,
.canvas-last>image
{
  width:100%;
  height:100%;
}
.canvas-second ec-canvas,
.canvas-third ec-canvas,
.canvas-last ec-canvas{
  width:100%;
  height:500rpx;
  position: absolute;
  top:200rpx;
  left:0;
  z-index: 666;
  color:#fff;
}

