.photo-list {
  padding: 30rpx 20rpx;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content:flex-start;
  flex-wrap: wrap;
}
.photo-item {
  width: 32%;
  height: 240rpx;
  margin:0 5rpx 16rpx;
}
.photo-item image {
  width: 100%;
  height: 100%;
}
.show{
  display:block;
}
.hide{
  display:none;
}
.model{
  position: fixed;
  top:0;
  left:0;
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,1);
}
.model image{
  width:100%;
  position: absolute;
  top:0;
  right:0;
  bottom:0;
  left:0;
  margin:auto;
}
