
.notice{
  padding:30rpx 36rpx;
  box-sizing: border-box;
}
.msg{
  width:100%;
  height:100rpx;
  line-height: 100rpx;
}
.msg>view{
  height:100%;
  border-bottom:1px solid#eee;
  border-top:1px solid#eee;
  background-color:#fff;
}
.msg>.left{
  width:70%;
}
.msg>.left input{
  width:100%;
  height:100%;
  padding-left:30rpx;
  box-sizing: border-box;
}
.msg>.right{
  text-align: center;
  color:#adadad;
  border-left:3px solid #eee;
  width:30%;
  padding-right:20rpx;
  box-sizing: border-box;
}
.btn{
  width:100%;
  height:100rpx;
  line-height: 100rpx;
  margin-top:100rpx;
}
.btn>button{
  width:90%;
  height:80rpx;
  line-height: 80rpx;
  letter-spacing: 0.2rem;
  background-color:#FD3B3B;
}