<view class="container-body">
  <view class="update">
    <view class="label">新密码</view>
    <view>
      <input type="password" placeholder="8-16位,至少含数字/字母/字符2种组合"/>
    </view>
    <view class="label">确认密码</view>
    <view>
      <input type="password" placeholder="8-16位,至少含数字/字母/字符2种组合"/>
    </view>
    <view>
      <button type="primary" size="default" bindtap='update'>确定</button>
    </view>
  </view>
</view>
