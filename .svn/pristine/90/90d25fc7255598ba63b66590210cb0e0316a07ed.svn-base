<view class="container-body">
  <view class="top-bg">
    <image src='https://small.aisuoedu.com/mini/img/user/8.png'/>
    <view>
      <image data-img="{{imgUrl[0]}}" src='{{imgUrl[0]}}'/>
      <view class="share">码上分享</view>
    </view>
  </view>
  <view class="bottom-list">
    <view>
      共分享<text>{{num}}</text>次
    </view>
    <view class="no-img">
      <image  wx:if="{{shareList.length==0}}" src="../../img/6.png"/>
    </view>
    <view class="share-list" wx:if="{{shareList.length>0}}">
      <view class="share-item clear" wx:for="{{shareList}}" wx:key="{{index}}">
        <view class="lf">2018-05-21</view>
        <view class="rf">14:30</view>
      </view>
    </view>
  </view>
</view>
