/**app.wxss**/
page{
  background-color: #fbfbfb;
  height:100%;
  font-size:30rpx;
  color:#333;
}
.clear:after{
  display:block;
  content:'';
  clear:both;
}
.container {
  width:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
} 
.container-body{
  width:100%;
  height:100%;
  margin:0 auto;
}
.lf{
  float: left;

}
.rf{
  float:right;
  text-align: right;
}
.common-img{
  height:30rpx;
  width:30rpx;
  margin:0 20rpx;
}
.icon-img{
  height:50rpx;
  width:50rpx;
  line-height: 50rpx;
  vertical-align: middle;
  margin:0 15rpx;
}
.no-img{
  width:80%;
  margin:0 auto;
}
.no-img>image{
  width:100%;
}
