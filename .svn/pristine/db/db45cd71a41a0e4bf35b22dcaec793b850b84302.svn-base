.container-body>view{
  padding:0 30rpx;
  box-sizing: border-box;
  font-size:32rpx;
}
.input{
  margin-top:30rpx;
  width:100%;
  height:80rpx;
  line-height: 80rpx;
  border:1px solid #eee;
  background-color:#fff; 
}
.input input{
  width:100%;
  height:100%;
  line-height: 80rpx;
}
.btn{
  margin-top:50rpx;
  box-sizing: border-box;
}
button{
  height:80rpx;
  line-height: 80rpx;
  letter-spacing: 0.2rem;
}